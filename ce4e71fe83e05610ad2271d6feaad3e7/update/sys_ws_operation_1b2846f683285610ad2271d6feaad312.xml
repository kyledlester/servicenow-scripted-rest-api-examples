        <operation_script><![CDATA[(function process( /*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

    var agg = new GlideAggregate('x_0294_rest_api_scripted_rest_api_basic_app');
    agg.addAggregate('COUNT', 'type');
    agg.orderBy('type');
    agg.query();

	var returnObj = {};

    while (agg.next()) {
        var incidentCount = agg.getAggregate('COUNT', 'type');
		var type = agg.type;
		returnObj[type] = incidentCount;
    }

    return returnObj;



})(request, response);]]></operation_script>
